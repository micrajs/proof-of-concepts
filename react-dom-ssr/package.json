{
  "name": "@micra/react-dom-ssr",
  "version": "0.1.0",
  "author": "Olavo Amorim Santos<olavo.santos@micra.dev>",
  "license": "MIT",
  "scripts": {
    "build:server": "vite build --config .viterc.ts",
    "build:types": "tsc -p tsconfig.json",
    "build": "concurrently 'pnpm:build:*'",
    "dev:server": "nodemon --ext 'ts,tsx' --exec 'NODE_ENV=development vite-node index.server.ts --config .viterc.ts'",
    "dev:types": "nodemon --ext 'ts,tsx' --exec 'pnpm build:types'",
    "dev": "concurrently 'pnpm:dev:*'",
    "lint": "eslint --ext .ts,.tsx .",
    "test:coverage": "vitest run --coverage --config .viterc.ts --passWithNoTests",
    "test:dev": "vitest --config .viterc.ts",
    "test": "vitest run --config .viterc.ts"
  },
  "devDependencies": {
    "@micra/core": "next",
    "@micra/eslint-config": "next",
    "@micra/typescript-config": "next",
    "@micra/vite-config": "next",
    "@types/node": "^17.0.21",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-react": "^1.3.2",
    "c8": "^7.11.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.11.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.0",
    "sass": "^1.53.0",
    "tsc-alias": "^1.6.4",
    "typescript": "^4.6.2",
    "vite-node": "^0.10.0",
    "vitest": "^0.6.1"
  },
  "dependencies": {
    "@micra/application": "next",
    "@micra/error": "next",
    "@micra/event-emitter": "next",
    "@micra/express-kernel": "next",
    "@micra/react-dom-ui-engine": "next",
    "@micra/react-router": "next",
    "@micra/request-handler": "next",
    "@micra/rest-router": "next",
    "@micra/router": "next",
    "@micra/spa-kernel": "next",
    "@micra/spa-router": "next",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^2.9.12"
  }
}
